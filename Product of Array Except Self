#include <stdio.h>

void pp(int a[] ,int n,int res[]){
  for(int i=0;i<n;i++){
    res[i]=1;
  }
  for(int i=0;i<n;i++){
    for(int j=0;j<n;j++){
      if(i!=j){
        res[i]*=a[j];
      }
    }
  }
}
int main() {
  int arr[] = {10, 3, 5, 6, 2};
  int n = sizeof(arr) / sizeof(arr[0]);
  int res[n];
  pp(arr, n, res);
  for (int i = 0; i < n; i++) {
    printf("%d ", res[i]);
  }
    
}

