#include <stdio.h>

int maxSubarraySum(int arr[],int n){
  int ms=arr[0];
  for(int i=0;i<n;i++){
    int cs=0;
    for(int j=0;j<n;j++){
      cs=cs+arr[j];
      if(cs>ms){
        ms=cs;
      }
    }
  }
  return ms;
}
int main() {
  int arr[] = {2, 3, -8, 7, -1, 2, 3};
  int n = sizeof(arr) / sizeof(arr[0]);
  printf("%d", maxSubarraySum(arr,n));
  return 0;
}

