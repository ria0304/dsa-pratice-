#include <stdio.h>
#include <stdbool.h>

#define MAX 100

int main() {
    int m, n;
    printf("Enter rows and columns: ");
    scanf("%d %d", &m, &n);

    int mat[MAX][MAX];
    printf("Enter the matrix elements:\n");
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &mat[i][j]);
        }
    }

    int res[MAX * MAX];
    bool vis[MAX][MAX] = {false}; 
    int dr[] = {0, 1, 0, -1};

    int dc[] = {1, 0, -1, 0};


    int r = 0, c = 0;


    int idx = 0;

    int count = 0;
    while (count < m * n) {
        res[count++] = mat[r][c];
        vis[r][c] = true;
        int newR = r + dr[idx];
        int newC = c + dc[idx];
        if (newR >= 0 && newR < m && newC >= 0 && newC < n && !vis[newR][newC]) {
            r = newR;
  
            c = newC;
        } else {
            idx = (idx + 1) % 4;
            r += dr[idx];
            c += dc[idx];
        }
    }

    for (int i = 0; i < m * n; i++) {
        printf("%d ", res[i]);
    }
    printf("\n");

    return 0;
}
